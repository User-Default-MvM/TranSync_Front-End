# Production Environment Variables for TransSync
# Copy this file and update with your actual production values

# API Configuration
REACT_APP_API_URL=https://transyncbackend-production.up.railway.app
REACT_APP_WS_URL=wss://api.transsync.com

# Application Configuration
REACT_APP_NAME=TransSync
REACT_APP_VERSION=1.1.0

# Geographic Configuration
REACT_APP_GEOLOCATION_TIMEOUT=15000
REACT_APP_GEOLOCATION_HIGH_ACCURACY=true

# External Services (update with your actual URLs)
REACT_APP_GOOGLE_MAPS_URL=https://www.google.com/maps
REACT_APP_OSM_URL=https://www.openstreetmap.org

# Production Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_DEFAULT_COUNTRY=co

# Analytics and Monitoring (add your actual tracking IDs)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Performance Settings
REACT_APP_IMAGE_COMPRESSION_QUALITY=0.8
REACT_APP_LAZY_LOAD_THRESHOLD=100